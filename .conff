# Virtual Hosts
#
<VirtualHost *:80>
ServerName localhost
ServerAlias localhost
DocumentRoot "${INSTALL_DIR}/www"
<Directory "${INSTALL_DIR}/www/">
Options +Indexes +Includes +FollowSymLinks +MultiViews
AllowOverride All

Require local
#Require all granted
</Directory>
</VirtualHost>

<VirtualHost *:80>
ServerName app.sfperusac.com
DocumentRoot "c:/wamp64/www/app.sfperusac"
<Directory "c:/wamp64/www/app.sfperusac/">
Options +Indexes +Includes +FollowSymLinks +MultiViews
AllowOverride All
#Header set Access-Control-Allow-Origin "*"
Require all granted
#Require local

</Directory>
</VirtualHost>

<VirtualHost *:80>
ServerName api.sfperusac.com
DocumentRoot "c:/wamp64/www/api.sfperusac"
<Directory "c:/wamp64/www/api.sfperusac/">
Options +Indexes +Includes +FollowSymLinks +MultiViews
AllowOverride All
Require all granted
#Require local

</Directory>
</VirtualHost>